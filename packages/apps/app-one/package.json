    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/setupTests.{js,jsx}",
      "!src/index.{js,jsx}",
      "!src/serviceWorker.{js,jsx}",
      "!src/**/*.(spec|test|stories).{js,jsx}"
    ]
  }
